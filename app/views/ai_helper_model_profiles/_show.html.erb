<hr/>
<div class="contextual">
    <%= link_to(sprite_icon('edit', l(:button_edit)), ai_helper_model_profiles_edit_path(@model_profile), :class => 'icon icon-edit') %>

    <%= link_to(sprite_icon('del', l(:button_delete)), ai_helper_model_profiles_destroy_url(id: @model_profile), method: :delete, data: { confirm: l('ai_helper.model_profiles.messages.confirm_delete') }, :class => 'icon icon-del') %>

</div>
<p>
  <%= label_tag "Llm type" %>
  <%= @model_profile.llm_type %>
</p>
<p>
  <%= label_tag "Access key" %>
  <%= @model_profile.masked_access_key %>
</p>
<p>
  <%= label_tag l(:field_llm_model) %>
  <%= @model_profile.llm_model %>
</p>
